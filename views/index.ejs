<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Expense Tracker</title>
</head>
<body>
  <h1>Custom Web Page</h1>

  <h2>Web Page Content</h2>
  <ul>
    <% expenses.forEach(expense => { %>
      <li><%= expense.category %> - $<%= expense.amount.toFixed(2) %> - <%= expense.date.toISOString().split('T')[0] %> - <%= expense.description || 'No description' %></li>
    <% }); %>
  </ul>
</body>
</html>
